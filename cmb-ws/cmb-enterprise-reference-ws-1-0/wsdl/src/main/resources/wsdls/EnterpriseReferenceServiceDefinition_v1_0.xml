		<OutputType>getUnitTypesResponse</OutputType>
		<MessageExchangePattern>request-response</MessageExchangePattern>
	</Operation>
	<Operation>
		<OperationName>getProvinces</OperationName>
		<Documentation>Retrieves Province List.</Documentation>
		<InputType wsp="False">getProvinces</InputType>
		<OutputType>getProvincesResponse</OutputType>
		<MessageExchangePattern>request-response</MessageExchangePattern>
	</Operation>
	<Operation>
		<OperationName>getStates</OperationName>
		<Documentation>Retrieves State List.</Documentation>
		<InputType wsp="False">getStates</InputType>
		<OutputType>getStatesResponse</OutputType>
		<MessageExchangePattern>request-response</MessageExchangePattern>
	</Operation>
	<Operation>
		<OperationName>getCountries</OperationName>
		<Documentation>Retrieves Countries.</Documentation>
		<InputType wsp="False">getCountries</InputType>
		<OutputType>getCountriesResponse</OutputType>
		<MessageExchangePattern>request-response</MessageExchangePattern>
	</Operation>
	<Operation>
		<OperationName>getLanguages</OperationName>
		<Documentation>Retrieves Languages.</Documentation>
