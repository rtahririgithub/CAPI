<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="applicationContext" class="org.springframework.context.support.ClassPathXmlApplicationContext">
		<constructor-arg>
			<list>
				<value>application-context-datasources-jndi.xml</value>
				<value>application-context-dao-jdbc.xml</value>
				<value>application-context-dao-ws.xml</value>
				<value>application-context-services.xml</value>
			</list>
		</constructor-arg>
	</bean>
</beans>